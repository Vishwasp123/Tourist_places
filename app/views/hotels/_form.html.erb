<%= form_with model: @hotel do |form|%>
	<div class="form-outline">
		<%= form.label :name, "Hotel name", class:"form-label" %>
		<%= form.text_field :name, placeholder:"Hotel name", class:"form-control my-2"%>
		<%= form.label :city, "City",class:"form-label"%>
		<%= form.text_field :city, placeholder: "City", class:"form-control my-2"%>
		<%= form.label :images,class:"form-label"%>
		<%= form.file_field :hotel_images,multiple: true, class:"form-control" %>
		<%= form.submit class:"btn btn-success mt-2" %>
	</div>
<% end %>